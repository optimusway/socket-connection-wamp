{
  "name": "ws-connection",
  "version": "2.0.6",
  "description": "",
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "test": "jest",
    "compile": "tsc --emitDeclarationOnly",
    "build": "tsc -p . && rollup -c",
    "start": "npm run-script compile && npm run-script build",
    "publish": "yarn build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/autobahn": "^0.9.39",
    "@types/jest": "^23.3.0",
    "husky": "^0.14.3",
    "jest": "^23.4.1",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "rollup": "^0.63.4",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^4.0.0",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "^2.9.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json,css,scss,sass}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{ts,tsx}": "tslint --fix"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/src/__tests__/.*)",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ]
  }
}
